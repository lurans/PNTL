<?xml version="1.0" encoding="UTF-8"?>
<multi-sso>

	<sso.multi.policy.class>com.huawei.sso.client.multiplane.DefaultMultiplaneConfig</sso.multi.policy.class>

	<sso.multi.planes>

		<sso.multi.plane url-pattern="192.168.0.0">
			<sso.multi.systemId>10002</sso.multi.systemId>
			<sso.multi.server.public.url>http://192.168.0.0:19991/sso</sso.multi.server.public.url>
			<sso.multi.server.private.url>http://192.168.0.0:19991/sso</sso.multi.server.private.url>
		</sso.multi.plane>

		<sso.multi.plane url-pattern="www.huawei-SOA.com">
			<sso.multi.systemId>10003</sso.multi.systemId>
			<sso.multi.server.public.url>http://192.168.0.0:9999/sso</sso.multi.server.public.url>
			<sso.multi.server.private.url>http://192.168.0.0:9999/sso</sso.multi.server.private.url>
		</sso.multi.plane>
	</sso.multi.planes>
</multi-sso>